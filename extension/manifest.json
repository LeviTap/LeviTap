{
    "manifest_version": 2,
    "name": "Gesture Control Extension",
    "version": "1.0",
    "description": "Chrome extension to control gestures using Python",
    "permissions": ["http://localhost/"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }
  